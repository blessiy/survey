<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="5dp"
    android:paddingRight="5dp"
    tools:context=".reportActivity"
    xmlns:app="http://schemas.android.com/apk/res-auto">

<TextView
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintLeft_toLeftOf="parent"
    android:layout_marginTop="5dp"
    android:textSize="13sp"
    android:id="@+id/question1"
    android:text="1.Which brand of your phone will you choose if yoou are going to buy a new one?"
    />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="10sp"
        android:id="@+id/answer1"
        app:layout_constraintTop_toBottomOf="@+id/question1"
        app:layout_constraintLeft_toLeftOf="@+id/question1"
        />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="13sp"
        android:id="@+id/question2"
        android:text="2.How much does your phone cost?"
        app:layout_constraintTop_toBottomOf="@+id/answer1"
        app:layout_constraintLeft_toLeftOf="@+id/answer1"
        android:layout_marginTop="5dp"
        />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="10sp"
        android:id="@+id/answer2"
        app:layout_constraintTop_toBottomOf="@+id/question2"
        app:layout_constraintLeft_toLeftOf="@+id/question2"

        />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="13sp"
        android:id="@+id/question3"
        android:text="3.What kind of phone are you using?"
        app:layout_constraintTop_toBottomOf="@+id/answer2"
        app:layout_constraintLeft_toLeftOf="@+id/answer2"
        android:layout_marginTop="5dp"
        />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="10sp"
        android:id="@+id/answer3"
        app:layout_constraintTop_toBottomOf="@+id/question3"
        app:layout_constraintLeft_toLeftOf="@+id/question3"
      />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="13sp"
        android:id="@+id/question4"
        android:text="4.What functions does your phone have now?"
        app:layout_constraintTop_toBottomOf="@+id/answer3"
        app:layout_constraintLeft_toLeftOf="@+id/answer3"
        android:layout_marginTop="5dp"
        />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="10sp"
        android:id="@+id/answer4"
        app:layout_constraintTop_toBottomOf="@+id/question4"
        app:layout_constraintLeft_toLeftOf="@+id/question4"
        />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="13sp"
        android:id="@+id/question5"
        android:text="5.What functions are the most often used?"
        app:layout_constraintTop_toBottomOf="@+id/answer4"
        app:layout_constraintLeft_toLeftOf="@+id/answer4"
        android:layout_marginTop="5dp"
        />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="10sp"
        android:id="@+id/answer5"
        app:layout_constraintTop_toBottomOf="@+id/question5"
        app:layout_constraintLeft_toLeftOf="@+id/question5"
        />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="13sp"
        android:id="@+id/question6"
        android:text="6.What functions do you expect your phone to have in the future?"
        app:layout_constraintTop_toBottomOf="@+id/answer5"
        app:layout_constraintLeft_toLeftOf="@+id/answer5"
        android:layout_marginTop="5dp"
        />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="10sp"
        android:id="@+id/answer6"
        app:layout_constraintTop_toBottomOf="@+id/question6"
        app:layout_constraintLeft_toLeftOf="@+id/question6"
        />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="13sp"
        android:id="@+id/question7"
        android:text="7.In which condiction will you buy a new phone?"
        app:layout_constraintTop_toBottomOf="@+id/answer6"
        app:layout_constraintLeft_toLeftOf="@+id/answer6"
        android:layout_marginTop="5dp"
        />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="10sp"
        android:id="@+id/answer7"
        app:layout_constraintTop_toBottomOf="@+id/question7"
        app:layout_constraintLeft_toLeftOf="@+id/question7"
        />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="13sp"
        android:id="@+id/question8"
        android:text="8.Which brand of your phone will you choose if you are going to buy a new one?"
        app:layout_constraintTop_toBottomOf="@+id/answer7"
        app:layout_constraintLeft_toLeftOf="@+id/answer7"
        android:layout_marginTop="5dp"
        />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="10sp"
        android:id="@+id/answer8"
        app:layout_constraintTop_toBottomOf="@+id/question8"
        app:layout_constraintLeft_toLeftOf="@+id/question8"
        />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="13sp"
        android:id="@+id/question9"
        android:text="9.What characteristic do you think is most important to evaluate a phone?"
        app:layout_constraintTop_toBottomOf="@+id/answer8"
        app:layout_constraintLeft_toLeftOf="@+id/answer8"
        android:layout_marginTop="5dp"
        />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="10sp"
        android:id="@+id/answer9"
        app:layout_constraintTop_toBottomOf="@+id/question9"
        app:layout_constraintLeft_toLeftOf="@+id/question9"
        />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="13sp"
        android:id="@+id/question10"
        android:text="10.How old are you?"
        app:layout_constraintTop_toBottomOf="@+id/answer9"
        app:layout_constraintLeft_toLeftOf="@+id/answer9"
        android:layout_marginTop="5dp"
        />
    <TextView
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    android:textSize="10sp"
    android:id="@+id/answer10"
        app:layout_constraintTop_toBottomOf="@+id/question10"
        app:layout_constraintLeft_toLeftOf="@+id/question10"
    />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="13sp"
        android:id="@+id/question11"
        android:text="11.What's your gender?"
        app:layout_constraintTop_toBottomOf="@+id/answer10"
        app:layout_constraintLeft_toLeftOf="@+id/answer10"
        android:layout_marginTop="5dp"
        />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="10sp"
        android:id="@+id/answer11"
        app:layout_constraintTop_toBottomOf="@+id/question11"
        app:layout_constraintLeft_toLeftOf="@+id/question11"
        />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="13sp"
        android:id="@+id/question12"
        android:text="12.How much money do you earn per month?"
        app:layout_constraintTop_toBottomOf="@+id/answer11"
        app:layout_constraintLeft_toLeftOf="@+id/answer11"
        android:layout_marginTop="5dp"
        />
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="10sp"
        android:id="@+id/answer12"
        app:layout_constraintTop_toBottomOf="@+id/question12"
        app:layout_constraintLeft_toLeftOf="@+id/question12"
        />
    <Button
        android:id="@+id/button"
        android:layout_height="60dp"
        android:layout_width="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:layout_gravity="center"
        android:text="FINISH"
        android:textSize="20sp"
        android:theme="@style/MyButton"

        />
</androidx.constraintlayout.widget.ConstraintLayout>